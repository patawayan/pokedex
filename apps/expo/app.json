{
  "expo": {
    "name": "pokedex",
    "slug": "pokedex",
    "scheme": "patawayan.pokedex",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/pokedex.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/pokedex.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.patawayan.pokedex.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/pokedex.png",
        "backgroundColor": "#eeeee"
      },
      "package": "com.patawayan.pokedex.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"]
  }
}
